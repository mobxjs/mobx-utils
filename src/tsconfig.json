{
    "version": "1.8.10",
    "compilerOptions": {
        "target": "es5",
        "sourceMap": false,
        "declaration": true,
        "module": "commonjs",
        "removeComments": false,
        "outDir": "../lib/",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "lib": [
            "dom", "es5", "scripthost", "es2015.promise"
        ]
    },
    "exclude": [
        "node_modules"
    ]
}
