{
    "version": "1.8.10",
    "compilerOptions": {
        "target": "es5",
        "sourceMap": false,
        "declaration": true,
        "module": "es6",
        "removeComments": false,
        "outDir": "../lib/",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "lib": [
            "dom", "es2015", "scripthost", "es2015.promise", "es2015.generator", "es2015.iterable"
        ]
    },
    "exclude": [
        "node_modules"
    ]
}
